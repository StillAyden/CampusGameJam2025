.button-unpressed:hover {
    scale: 1.1 1.1;
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out-cubic;
}

.minimize-button:hover {
    scale: 1.1 1.1;
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out-cubic;
}

.button-unpressed:active {
    background-color: rgb(253, 253, 253);
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out-cubic;
    scale: 0.9 0.9;
}

.minimize-button:active {
    background-color: rgb(253, 253, 253);
    transition-duration: 0.2s;
    transition-timing-function: ease-in-out-cubic;
    scale: 0.9 0.9;
}

